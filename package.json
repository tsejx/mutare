{
  "name": "mutare",
  "version": "0.2.0",
  "description": "Some pratical utilities function for JavaScript Developer.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "dist/mutare.min.js",
  "typings": "lib/index.d.ts",
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "scripts": {
    "docs": "yarn run dumi dev --port=6547",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "babel package --out-dir es",
    "build:umd": "cross-env NODE_ENV=development rollup -c -o dist/mutare.js",
    "build:umd:min": "cross-env NODE_ENV=production rollup -c -o dist/mutare.min.js",
    "build": "npm run clean && NODE_ENV=prod rollup -c scripts/rollup.config.js",
    "clean": "rimraf ./lib ./es ./dist",
    "test": "mocha --require @babel/register ./package/*/__tests__/*.test.js --recursive",
    "lint": "eslint --ext .js src",
    "version": "git add CHANGELOG.MD",
    "prettier": "node ./scripts/prettier.js",
    "coverage": "codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsejx/mutare.git"
  },
  "keywords": [
    "JavaScript",
    "JS",
    "js",
    "toolkit",
    "function",
    "utils",
    "package",
    "mutare"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tsejx/mutare/issues"
  },
  "homepage": "https://github.com/tsejx/mutare#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "@typescript-eslint/parser": "^4.29.0",
    "chai": "^4.2.0",
    "chalk": "^2.4.2",
    "commander": "^2.20.0",
    "cross-env": "^7.0.2",
    "dumi": "^1.0.34",
    "eslint": "^5.12.0",
    "eslint-plugin-import": "^2.15.0",
    "glob": "^7.1.3",
    "mocha": "^5.2.0",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "rollup": "^1.1.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-prettier": "^0.6.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-strip-banner": "^0.2.0",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "rollup-plugin-uglify": "^6.0.2",
    "semantic-release": "^17.1.1",
    "typescript": "^4.3.5",
    "uglify-es": "^3.3.9"
  },
  "dependencies": {},
  "author": "tsejx <tsejx@foxmail.com>"
}
